{
  "name": "event-protocal-server",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [],
  "type": "module",
  "scripts": {
    "release": "tsc --noEmit && rm -rf dist && swc --strip-leading-paths -d dist src",
    "start": "node --env-file=./.env.local dist/entrypoints/server.js",
    "dev": "nodemon",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --cache --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma": "prisma",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "check": "pnpm type-check && pnpm lint",
    "build:tsc": "rm -rf dist && tsc && tsc-alias"
  },
  "dependencies": {
    "@fastify/autoload": "^5.10.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.0.1",
    "@prisma/client": "5.17.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^2.0.0",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cspell/eslint-plugin": "^8.13.0",
    "@eslint/js": "^9.8.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.3",
    "@types/lodash": "^4.17.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "5.17.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0"
  },
  "engines": {
    "node": ">=20.16.0",
    "pnpm": ">=9"
  }
}
